<?xml version="1.0" encoding="utf-8"?>
<extension version="2.5" type="plugin" group="jevents" method="upgrade">
	<name>JEvents - Category and Calendar Viewing Restrictions</name>
	<author>Geraint Edwards</author>
	<creationDate>June 2012</creationDate>
	<copyright>(C) 2010-1012 GWE Systems Ltd. All rights reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail></authorEmail>
	<authorUrl>www.gwesystems.com</authorUrl>
	<version>3.0</version>
	<description>JEV_CAT_CAL_DESC</description>
	<files>
		<filename plugin="jevcatcal">jevcatcal.php</filename>
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.plg_jevents_jevcatcal.ini</language>
		<language tag="en-GB">en-GB/en-GB.plg_jevents_jevcatcal.sys.ini</language>
   	</languages>		
	<config>
		<fields name="params">
			<fieldset name="basic"  >
				<field name="whitelist" type="text" size="40" default="" label="JEV_WHITELIST_CATS" description="JEV_WHITELIST_CATS_DESC" />
			</fieldset>
		</fields>
	</config>
</extension>
